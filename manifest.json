{
    "name": "Page Observer",
    "description" : "",
    "version": "0.0",
    "manifest_version": 2,
    "content_scripts" : [
	{
	    "matches" : ["*://*/*"],
	    "js" : ["content.js"]
	}
    ],
    "browser_action" : {
	"default_popup" : "popup.html"
    },
    "permissions" : ["notifications"]
}
